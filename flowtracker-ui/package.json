{
  "name": "flowtracker-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint \"src/**/*.{ts,js,svelte}\"",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "validate": "npx prettier --write . && npm run check && npm run lint && npm run test"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/svelte": "^4.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@tsconfig/svelte": "^5.0.2",
    "@vitest/ui": "^1.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-svelte": "^2.35.1",
    "jsdom": "^24.0.0",
    "msw": "^2.2.7",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.4",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.3.1",
    "vite": "^5.0.8",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "svelte-splitpanes": "^0.8.0"
  }
